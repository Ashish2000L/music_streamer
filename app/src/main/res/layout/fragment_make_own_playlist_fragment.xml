        android:layout_height="wrap_content"
        android:padding="5dp"
        tools:text="Hello world"
        android:textAlignment="center"
        android:textSize="25sp"
        android:layout_marginTop="20dp"
        android:filterTouchesWhenObscured="true"
        />

    </RelativeLayout>

</FrameLayout>